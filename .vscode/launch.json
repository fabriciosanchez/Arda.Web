{
    "version": "0.2.0",
    "configurations": [
    {
        "name": ".NET Core Launch (web)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "/Users/fabricio/Documents/Microsoft/Projects/Arda.Web/Arda.Web/Arda.Web/bin/Debug/netcoreapp1.1/Arda.Web.dll",
        "args": [],
